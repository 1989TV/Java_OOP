package OOP_Inheritence;

interface Shape{

	public double area();

	public double perimeter();
}

public class Cirlce implements Shape {
	double radius;
	
	Cirlce(double radius){
		this.radius = radius;
	}

	@Override
	public double area() {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public double perimeter() {
		// TODO Auto-generated method stub
		return 0;
	}

}
